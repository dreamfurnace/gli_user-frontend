# GLI User Frontend - Local Environment Variables Template
# Copy this file to .env.local and fill in your local values

# Local Development Override Settings
# These variables will override the environment-specific settings

# API Configuration (if different from development)
# VITE_API_BASE_URL=http://localhost:8000

# Solana Configuration (for local testing)
# VITE_SOLANA_NETWORK=devnet
# VITE_SOLANA_RPC_URL=https://api.devnet.solana.com

# Development Tools
# VITE_ENABLE_DEVTOOLS=true
# VITE_ENABLE_DEBUG_MODE=true

# Local Feature Testing
# VITE_TEST_WALLET_ADDRESS=your_test_wallet_address_here
# VITE_LOCAL_STORAGE_DEBUG=true

# Performance Testing
# VITE_PERFORMANCE_BUDGET_JS=250000
# VITE_PERFORMANCE_BUDGET_CSS=100000