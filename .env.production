# GLI User Frontend Production Environment

# API Configuration
VITE_API_BASE_URL=https://api.gli-platform.com
VITE_API_VERSION=v1
VITE_API_TIMEOUT=20000

# Solana Configuration
VITE_SOLANA_NETWORK=mainnet-beta
VITE_SOLANA_RPC_URL=https://api.mainnet-beta.solana.com
VITE_SOLANA_COMMITMENT=finalized

# Authentication
VITE_JWT_STORAGE_KEY=gli_auth_token
VITE_USER_PROFILE_STORAGE_KEY=gli_user_profile

# App Configuration
VITE_APP_TITLE=GLI User Frontend
VITE_APP_DESCRIPTION=GLI Platform User Frontend Application
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=production

# Feature Flags
VITE_ENABLE_DEVTOOLS=false
VITE_ENABLE_DEBUG_MODE=false
VITE_ENABLE_PERFORMANCE_MONITOR=false
VITE_ENABLE_ERROR_REPORTING=true

# PWA Settings
VITE_PWA_ENABLED=true
VITE_PWA_CACHE_NAME=gli-user-frontend-production-cache

# Logging
VITE_LOG_LEVEL=error
VITE_ENABLE_CONSOLE_LOGS=false

# Production Optimizations
VITE_HOT_RELOAD=false
VITE_SOURCE_MAPS=false
VITE_BUILD_OPTIMIZATION=true

# Error Reporting
VITE_SENTRY_DSN=
VITE_ERROR_REPORTING_ENDPOINT=

# Analytics
VITE_GOOGLE_ANALYTICS_ID=
VITE_MIXPANEL_TOKEN=

# CDN Settings
VITE_CDN_URL=https://cdn.gli-platform.com
VITE_STATIC_ASSETS_URL=https://static.gli-platform.com